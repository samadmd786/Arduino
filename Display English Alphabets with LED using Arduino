  digitalWrite(ML, LOW);
  digitalWrite(MM, LOW);
  digitalWrite(MR, LOW);
  digitalWrite(LL, LOW);
  digitalWrite(LM, LOW);
  digitalWrite(LR, LOW); 
 

 
  digitalWrite(UL, HIGH);
  digitalWrite(UM, HIGH);
  digitalWrite(UR, HIGH);
  digitalWrite(ML, HIGH);
  digitalWrite(MM, HIGH);
  digitalWrite(MR, HIGH);
  digitalWrite(LL, HIGH);
  digitalWrite(LM, HIGH);
  digitalWrite(LR, HIGH);
