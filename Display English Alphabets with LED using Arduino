//Display English Alphabets with LED using Arduino

int UL = 2;
int UM = 3;
int UR = 4;
int ML = 5;
int MM = 6;
int MR = 7;
int LL = 8;
int LM = 9;
int LR = 10;
int timer = 1500;

void setup() {
  // put your setup code here, to run once:
  pinMode(UL, OUTPUT);
  pinMode(UM, OUTPUT);
  pinMode(UR, OUTPUT);
  pinMode(ML, OUTPUT);
  pinMode(MM, OUTPUT);
  pinMode(MR, OUTPUT);
  pinMode(LL, OUTPUT);
  pinMode(LM, OUTPUT);
  pinMode(LR, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:

/*
 `digitalWrite(UL, LOW);
